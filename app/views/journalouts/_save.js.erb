$("ul.errors").html("")
<% if @journalout.errors.any? %>
  <% @journalout.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".journalout-index").html("<%= escape_javascript(render 'index') %>")
  $("#journalout-modal").modal("hide")
<% end %>