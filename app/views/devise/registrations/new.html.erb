<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <div class="form-inputs">
    <%= f.input :email, placeholder: 'Ingrese su Email', as: :email  %>
    <%= f.input :firstName, placeholder: 'Nombres' %>
    <%= f.input :lastName, placeholder: 'Apellidos' %>
    <%= f.input :rut, placeholder: 'Ejemplo: 19358552-3'%>
    <%= f.input :contactPhone, placeholder: 'Ejemplo: 56 2 27467123' %>
    <%= f.input :contactEmail, placeholder: 'contacto@dominio.com' %>
    <%= f.input :password , as: :password ,  autocomplete: "off"%>
    <%= f.input :password_confirmation , as: :password,  autocomplete: "off"%>
    <%= f.input :profile_id %>
    <%= f.input :community_id %>
    <%= f.input :communitypass %>
  </div>

  <div class="form-actions">
     <%= f.button :submit, class:"btn btn-success"%>
  </div>
<% end %>
<%= render "devise/shared/links" %>

</div>
</div>
</div>
</section>

                   
