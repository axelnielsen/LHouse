<div class="modal-dialog">
  <div class="modal-content">
    <%= form_for @service, remote: true, html: { style: "display:inline;" } do |f| %>
      <div class="modal-body">
        <ul class="errors"></ul>

        <div class="col-sm-12">
        <div class="form-group">
          <%= f.label :name, class: "control-label" %>
          <%= f.text_field :name, class: "form-control" %>
        </div>
  </div>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :period, class:"control-label" %>
          <%= f.text_field :period, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :porcgastocomun, class:"control-label" %>
          <%= f.text_field :porcgastocomun, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :porcaguacaliente, class:"control-label" %>
          <%= f.text_field :porcaguacaliente, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :porccalefaccion, class:"control-label" %>
          <%= f.text_field :porccalefaccion, class: "form-control" %>
        </div>
</div>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :vendor_id, class:"control-label" %>
          <%= f.text_field :vendor_id, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :custnumber, class:"control-label" %>
          <%= f.text_field :custnumber, class: "form-control" %>
        </div>
</div>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :payableaccount_id, class: "control-label" %>
          <%= f.text_field :payableaccount_id, class: "form-control" %>
        </div>
</div>
     
      <div class="modal-footer">
        <%= f.submit class: "btn btn-success" %>
        <%= link_to "Cancelar", "#", class: "btn btn-danger", data: {dismiss: "modal"} %>
      </div>
    <% end %>
  </div>
</div>
