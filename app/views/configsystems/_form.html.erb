<div class="modal-dialog">
  <div class="modal-content">
    <%= form_for @configsystem, remote: true, html: { style: "display:inline;" } do |f| %>
      <div class="modal-body">
        <ul class="errors"></ul>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :interesttype_id, class:"control-label" %>
          <%= f.text_field :interesttype_id, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :interesttypeperc, class:"control-label" %>
          <%= f.text_field :interesttypeperc, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :interestreceivableaccount_id, class:"control-label" %>
          <%= f.text_field :interestreceivableaccount_id, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :penaltyamount, class:"control-label" %>
          <%= f.text_field :penaltyamount, class: "form-control" %>
        </div>
</div>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :penaltyreceivableaccount_id, class:"control-label" %>
          <%= f.text_field :penaltyreceivableaccount_id, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :periodmonth, class:"control-label" %>
          <%= f.text_field :periodmonth, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :periodinit, class: "control-label" %>
          <%= f.text_field :periodinit, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :periodfin, class: "control-label" %>
          <%= f.text_field :periodfin, class: "form-control" %>
        </div>
  </div>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :extension, class: "control-label" %>
          <%= f.text_field :extension, class: "form-control" %>
        </div>
          <div class="form-group">
          <%= f.label :correctionday, class: "control-label" %>
          <%= f.text_field :correctionday, class: "form-control" %>
        </div>
</div>
      </div>
      <div class="modal-footer">
        <%= f.submit class: "btn btn-success" %>
        <%= link_to "Cancelar", "#", class: "btn btn-danger", data: {dismiss: "modal"} %>
      </div>
    <% end %>
  </div>
</div>
