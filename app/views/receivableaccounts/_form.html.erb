<div class="modal-dialog">
  <div class="modal-content">
    <%= form_for @receivableaccount, remote: true, html: { style: "display:inline;" } do |f| %>
      <div class="modal-body">
        <ul class="errors"></ul>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :name, class:"control-label" %>
          <%= f.text_field :name, class: "form-control" %>
        </div>
             
</div>
<div class="col-sm-6">
         <div class="form-group">
          <%= f.label :initbalance, class:"control-label" %>
          <%= f.text_field :initbalance, class: "form-control" %>
        </div> 
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :descroption, class:"control-label" %>
          <%= f.text_field :descroption, class: "form-control" %>
        </div>       
</div>
<div class="col-sm-6">
         <div class="form-group">
          <%= f.label :pymenttype_id, class:"control-label" %>
          <%= f.text_field :pymenttype_id, class: "form-control" %>
        </div> 
        </div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :subtype, class:"control-label" %>
          <%= f.text_field :subtype, class: "form-control" %>
        </div>       
</div>
<div class="col-sm-6">
         <div class="form-group">
          <%= f.label :ammount, class:"control-label" %>
          <%= f.text_field :ammount, class: "form-control" %>
        </div> 
        </div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :percentammount, class:"control-label" %>
          <%= f.text_field :percentammount, class: "form-control" %>
        </div>       
</div>
<div class="col-sm-6">
         <div class="form-group">
          <%= f.label :aliquot, class:"control-label" %>
          <%= f.text_field :aliquot, class: "form-control" %>
        </div> 
        </div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :building, class:"control-label" %>
          <%= f.text_field :building, class: "form-control" %>
        </div>       
</div>



      </div>
      <div class="modal-footer">
        <%= f.submit class: "btn btn-success" %>
        <%= link_to "Cancelar", "#", class: "btn btn-danger", data: {dismiss: "modal"} %>
      </div>
    <% end %>
  </div>
</div>
