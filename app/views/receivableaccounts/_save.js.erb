$("ul.errors").html("")
<% if @receivableaccount.errors.any? %>
  <% @receivableaccount.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".receivableaccount-index").html("<%= escape_javascript(render 'index') %>")
  $("#receivableaccount-modal").modal("hide")
<% end %>