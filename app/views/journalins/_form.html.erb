<div class="modal-dialog">
  <div class="modal-content">
    <%= form_for @journalin, remote: true, html: { style: "display:inline;" } do |f| %>
      <div class="modal-body">
        <ul class="errors"></ul>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :name, class:"control-label" %>
          <%= f.text_field :name, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :voucher, class:"control-label" %>
          <%= f.text_field :voucher, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :receivableaccount_id, class:"control-label" %>
          <%= f.text_field :receivableaccount_id, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :amount, class:"control-label" %>
          <%= f.text_field :amount, class: "form-control" %>
        </div>
</div>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :paymentdate, class:"control-label" %>
          <%= f.text_field :paymentdate, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :apartment_id, class:"control-label" %>
          <%= f.text_field :apartment_id, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :paymentmethod_id, class: "control-label" %>
          <%= f.text_field :paymentmethod_id, class: "form-control" %>
        </div>
</div>
<div class="col-sm-6">
        <div class="form-group">
          <%= f.label :period, class: "control-label" %>
          <%= f.text_field :period, class: "form-control" %>
        </div>
  </div>
<div class="col-sm-12">
        <div class="form-group">
          <%= f.label :image, class: "control-label" %>
          <%= f.text_field :image, class: "form-control" %>
        </div>
</div>
      </div>
      <div class="modal-footer">
        <%= f.submit class: "btn btn-success" %>
        <%= link_to "Cancelar", "#", class: "btn btn-danger", data: {dismiss: "modal"} %>
      </div>
    <% end %>
  </div>
</div>
