<p id="notice"><%= notice %></p>

<p>
  <strong>Asunto:</strong>
  <%= @task.name %>
</p>

<p>
  <strong>Fecha Inicio:</strong>
  <%= @task.inidate %>
</p>

<p>
  <strong>Fecha Término:</strong>
  <%= @task.findate %>
</p>

<p>
  <strong>Hora Inicio:</strong>
  <%= @task.inihour %>
</p>

<p>
  <strong>Hora Término:</strong>
  <%= @task.finhour %>
</p>

<p>
  <strong>Detalle:</strong>
  <%= @task.content %>
</p>

<p>
  <strong>User id from:</strong>
  <%= @task.user_id_from %>
</p>

<p>
  <strong>User id to:</strong>
  <%= @task.user_id_to %>
</p>

<p>
  <strong>% Completado:</strong>
  <%= @task.perccompleted %>
</p>
<%= link_to 'Editar', edit_task_path(@task) , class:"btn btn-success"%>
<%= link_to 'Atrás', tasks_path, class:"btn btn-warning"%>

