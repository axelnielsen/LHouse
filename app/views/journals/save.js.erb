$("ul.errors").html("")
<% if @journal.errors.any? %>
  <% @journal.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".journal-index").html("<%= escape_javascript(render 'index') %>")
  $("#new_journal_modal").modal("hide")
<% end %>